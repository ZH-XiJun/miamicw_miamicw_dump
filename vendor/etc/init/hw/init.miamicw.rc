    write /sys/class/devfreq/ddrfreq_latency/min_freq 1622000000
    write /sys/class/devfreq/ddrfreq_latency/max_freq 1622000000
    write /sys/class/devfreq/ddrfreq_up_threshold/max_freq 1622000000
    write /sys/class/devfreq/ddrfreq_up_threshold/min_freq 1622000000


    write /proc/sys/kernel/hung_task_panic 1
