    write /sys/class/devfreq/ddrfreq_latency/min_freq 0
    write /sys/class/devfreq/ddrfreq_latency/max_freq 0
    write /sys/class/devfreq/ddrfreq_up_threshold/max_freq 1622000000
    write /sys/class/devfreq/ddrfreq_up_threshold/min_freq 1622000000

on charger
